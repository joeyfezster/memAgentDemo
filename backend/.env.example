# Environment Configuration Example
# Copy this file to .env and fill in your values

# Application Settings
PROJECT_NAME="memAgent Demo API"
ENVIRONMENT="local"  # local, development, staging, production
DEBUG=true

# Database Configuration
DATABASE_URL="postgresql+asyncpg://postgres:postgres@postgres:5432/postgres"

# Letta Agent Configuration
# Get your API key from: https://app.letta.com/settings
# REQUIRED for agent functionality
LETTA_API_KEY=""

# Optional: Custom Letta server URL
# Only needed if using self-hosted Letta instance
# Leave empty to use Letta's hosted service (default)
LETTA_BASE_URL=""

# Example values for reference:
# LETTA_API_KEY="sk-letta-abc123def456..."
# LETTA_BASE_URL="https://api.letta.com"  # or your self-hosted instance
